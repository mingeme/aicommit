{
  "name": "aicommit",
  "version": "0.1.0",
  "description": "AI-powered git commit message generator.",
  "main": "dist/index.js",
  "bin": {
    "aicommit": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "node scripts/check-version.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node scripts/check-version.js && ts-node src/index.ts",
    "version": "node scripts/generate-version.js",
    "test": "jest",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "git",
    "cli",
    "aicommit",
    "typescript"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.7",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "inquirer": "^12.4.2",
    "openai": "^4.86.1",
    "yaml": "^2.7.0"
  }
}